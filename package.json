    "split2": "^2.0.1"
  },
  "devDependencies": {
    "basic-auth": "^1.0.3",
    "basic-auth-connect": "^1.0.0",
    "bluebird": "^3.4.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.3.0",
    "express": "^4.13.3",
    "mocha": "^2.3.0",
